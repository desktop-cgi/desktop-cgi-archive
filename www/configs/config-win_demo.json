{
    "server": {
        "host": "localhost",
        "port": 3001,
        "app": "demo",
        "redirect_home": "/proxyone",
        "binaries": "/binaries",
        "options": {
            "viewengine": "ejs",
            "views": "/www/demoapp/node-templates",
            "assets": "/www/demoapp/ui/assets"
        }
    },
    "session": {
        "secret": "1234567890"
    },
    "app": {
        "temp": "/tmp",
        "cache": "/data/cache",
        "downloads": "/downloads",
        "userData": "/data/user",
        "logs": "/data/logs",
        "recent": "/data/recent",
        "crashDump": "/data/crash",
        "appData": "/data/app"
    },
    "processes": {
        "jspath": {
            "type": "nginx"
        }
    },
    "proxies": {
        "proxyone": {
            "type": "http",
            "options": {
                "target": {
                    "protocol": "http:",
                    "host": "127.0.0.1",
                    "port": 9001,
                    "pfx": null,
                    "passphrase": ""
                },
                "ws": false,
                "secure": false,
                "ssl": {
                    "key": "",
                    "cert": ""
                },
                "xfwd": true,
                "toProxy": true,
                "prependPath": true,
                "ignorePath": false,
                "changeOrigin": false,
                "preserveHeaderKeyCase": true,
                "auth": ":",
                "hostRewrite": true,
                "protocolRewrite": null,
                "cookieDomainRewrite": false,
                "cookiePathRewrite": false,
                "headers": {},
                "proxyTimeout": 10000,
                "timeout": 10000,
                "selfHandleResponse": false,
                "buffer": null
            },
            "listenPort": 8001,
            "stream": false,
            "modify": false,
            "runtime": false
        }
    },
    "cgifiles": {
        "wordpress": {
            "label": "PHP Wordpress",
            "identity": "phpwordpress",
            "path": "/wp",
            "lang_type": "php",
            "web_root_folder": "./www/demoapp/files/php",
            "bin": "",
            "config_path": "",
            "host": "localhost",
            "port": 3001,
            "cmd_options": {}
        },
        "ecommerce": {
